{% extends 'PiwiSystemCoreBundle::default.html.twig' %}

{% block title %}{{ parent() }}{% endblock %}

{% block head_style %}
    {{ parent() }}
    {% stylesheets filter="cssrewrite"
    '../src/Piwi/S2p/EventBundle/Resources/private/less/core.less'
    %}
    <link href="{{ asset_url }}" type="text/css" rel="stylesheet" media="screen" />
    {% endstylesheets %}
{% endblock %}

{% block foot_script_assetic %}
    {{ parent() }}
    {% javascripts combine=true
    '../src/Piwi/S2p/EventBundle/Resources/private/js/edit.js'
    '../src/Piwi/S2p/EventBundle/Resources/private/js/add.js'
    %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}

{% block content %}{% endblock %}
